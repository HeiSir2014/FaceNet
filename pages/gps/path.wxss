.container{
  background-color: #40A6FF;
  height: 100%;
  overflow: hidden;
}

label{
  display: block;
}

.head{
  width: 100rpx;
  height: 100rpx;
}

.loc{
  position: absolute;
  width: 100%;
  height: 100%;
  bottom: 120rpx;
}

.delete_mac {
  position: absolute;
  bottom: 20rpx;
  left: 5%;
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
}

.macSelect{
  position: absolute;
  left: 5%;
  width: 45%;
  height: 120rpx;
  line-height: 120rpx;
  bottom: 0rpx;
  text-align: center;
  color: #FFF;
}
.macSelect text{
  color: #EEE;
}
.dateSelect{
  position: absolute;
  left: 50%;
  width: 45%;
  height: 120rpx;
  line-height: 120rpx;
  bottom: 0rpx;
  text-align: center;
  color: #FFF;
  
}